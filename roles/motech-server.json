{
  "name": "motech-server",	

  "json_class": "Chef::Role",
  "env_run_lists": {
    "prod": [      
      "recipe[cron]",
      "recipe[curl]",
      
      "recipe[java]",
      
      "recipe[mysql::server]",
      "recipe[mysql::client]",
      "recipe[activemq]",
      "recipe[couchdb]",
      
      "recipe[apache2]",
