{
  "name": "motech-demo",	

  "json_class": "Chef::Role",
  "env_run_lists": {
    "prod": [      
      "role[motech-server]",
      	      
      "recipe[swapfile]",
      "recipe[maven]",
      "recipe[motech-demo]"
    ]
  },
  "run_list": [
  ],	

  "override_attributes": {
    "mysql": {
       "server_root_password": "password"
    },
    "swapfile": {
       "swap_size_megabytes": "1024"
    }
  },

  "default_attributes": {
    "motech_demo": {
       "hostname": "demo.motechproject.org"
    }
  }, 

  "description": "Sets up motech demo instance",
  "chef_type": "role"
}
